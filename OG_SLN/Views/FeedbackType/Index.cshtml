            Description
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.IS_ACTIVE)*@
            Active
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.FEEDBACK_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FEEDBACK_CODE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FEEDBACK_DESC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ACTIVE_STATUS)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FEEDBACK_TYPE_NO }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.FEEDBACK_TYPE_NO }) |*@
            @Html.ActionLink("Delete", "Delete", new { id=item.FEEDBACK_TYPE_NO })
