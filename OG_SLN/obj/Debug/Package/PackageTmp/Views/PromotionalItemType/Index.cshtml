@model IEnumerable<OG_SLN.SET_PROMO_ITEM_TYPE>
@{
    ViewBag.Title = "Index";
}
<h2>
    Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Type Name
        </th>
        <th>
            Type Code
        </th>
        <th>
            Type description
        </th>
        <th>
            Active
        </th>
        <th>
            Active From
        </th>
        <th>
            Active To
        </th>
        <th>
            Sl Num
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PROMO_ITEM_TYPE_NAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PROMO_ITEM_TYPE_CODE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PROMO_ITEM_TYPE_DESC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACTIVE_ITEM_TYPE)
            </td>
            <td>
                @Html.ValueFor(modelItem => item.ACTIVE_FROM, "{0:yyyy-MM-dd}")
            </td>
            <td>
                @Html.ValueFor(modelItem => item.ACTIVE_TO, "{0:yyyy-MM-dd}")
            </td>
            <td>
                @Html.ValueFor(modelItem => item.SL_NUM, "{0:0}")
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.PROMO_ITEM_TYPE_NO }, new { @class = "btn btn-info" })
                |
                @Html.ActionLink("Details", "Details", new { id = item.PROMO_ITEM_TYPE_NO }, new { @class = "btn btn-success" })
                |
                @Html.ActionLink("Delete", "Delete", new { id = item.PROMO_ITEM_TYPE_NO }, new { @class = "btn btn-danger" })
            </td>
        </tr>
    }
</table>
