@model OG_SLN.SET_BOOK_BRAND_TYPE
@{
    ViewBag.Title = "Create";
}
<h2>
    Create</h2>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    <fieldset>
    <legend>Book Brand Type</legend>
        <div class="new_FormDiv">
            
            <div class="editor-label">
                @Html.LabelFor(model => model.BOOK_BRAND_TYPE_NAME)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.BOOK_BRAND_TYPE_NAME)
                @Html.ValidationMessageFor(model => model.BOOK_BRAND_TYPE_NAME)
            </div>
            <div class="editor-label">
                @Html.LabelFor(model => model.DESC_BOOK_BRAND)
            </div>
            <div class="editor-field">
                @Html.TextAreaFor(model => model.DESC_BOOK_BRAND)
                @Html.ValidationMessageFor(model => model.DESC_BOOK_BRAND)
            </div>
            <div class="editor-label">
                @Html.LabelFor(model => model.ACTIVE_BRAND)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.ACTIVE_BRAND)
                @Html.ValidationMessageFor(model => model.ACTIVE_BRAND)
            </div>
            <div class="editor-label">
                @Html.LabelFor(model => model.ACTIVE_FROM)
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(model => model.ACTIVE_FROM, new { @class = "datesinspec" })
                @Html.ValidationMessageFor(model => model.ACTIVE_FROM)
            </div>
            <div class="editor-label">
                @Html.LabelFor(model => model.ACTIVE_TO)
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(model => model.ACTIVE_TO, new { @class = "datesinspec" })
                @Html.ValidationMessageFor(model => model.ACTIVE_TO)
            </div>
            <div class="editor-label">
                @Html.LabelFor(model => model.SL_NUM)
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(model => model.SL_NUM)
                @Html.ValidationMessageFor(model => model.SL_NUM)
            </div>
            <p>
                <input type="submit" value="Save" class="btn btn-success" />
            </p>
        </div>
    </fieldset>
}
<div>
    @Html.ActionLink("Back to List", "Index", new { }, new { @class = "btn btn-primary" })
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
