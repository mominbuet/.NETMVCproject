                    </td>

                    <td>
                        @*@Html.ActionLink("Edit", "Edit", new { id=item.FEED_BACK_NO }) |*@
                        @Html.ActionLink("View", "Details", new { id=item.FEED_BACK_NO }) 
                        @*@Html.ActionLink("Delete", "Delete", new { id=item.FEED_BACK_NO })*@
                    </td>
                </tr>
            }

            </table>
            </div>
        </div>
    </div>

<div id='Paging' style="text-align:center">
    Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber)
    of @Model.PageCount
    @Html.PagedListPager(Model, page => Url.Action("Index", new { page }))
</div>