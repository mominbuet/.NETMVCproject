@model IEnumerable<OG_SLN.SET_BOOK_PROJ_TYPE>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-responsive">
    <tr>
        
        <th>
            Project Type
        </th>
        <th>
            Type Description
        </th>
        
        <th>
            Active
        </th>
        <th>
            Active From
        </th>
        <th>
            Active To
        </th>
        <th>
            Serial No
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.BOOK_PROJ_TYPE_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BOOK_PROJ_TYPE_DESC)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.ACTIVE_PROJ_TYPE)
        </td>
        <td>
            @Html.ValueFor(modelItem => item.ACTIVE_FROM, "{0:yyyy-MM-dd}")
        </td>
        <td>
            @Html.ValueFor(modelItem => item.ACTIVE_TO, "{0:yyyy-MM-dd}")
        </td>
        <td>
            @Html.ValueFor(modelItem => item.SL_NUM,"{0:0}")
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.BOOK_PROJ_TYPE_NO }, new { @class="btn btn-info"}) |
            @Html.ActionLink("Details", "Details", new { id = item.BOOK_PROJ_TYPE_NO }, new { @class = "btn btn-success" }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.BOOK_PROJ_TYPE_NO }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
