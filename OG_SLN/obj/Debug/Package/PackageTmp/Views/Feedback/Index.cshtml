                    </th>

                    <th></th>
                </tr>

            @foreach (var item in Model) {
                <tr>
        
                    <td>
                        @Html.DisplayFor(modelItem => item.SEC_USERS.USER_FULL_NAME)
                    </td>
                    <td>
