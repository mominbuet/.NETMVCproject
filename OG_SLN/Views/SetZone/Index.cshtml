@model IEnumerable<OG_SLN.SET_ZONE>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-responsive">
    <tr>
        <th>
            Division
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ZONE_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ZONE_CODE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ZONE_DETAILS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESC_ZONE)
        </th>
       
        <th>
            Students
        </th>
         <th>
            Grade
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACTIVE_ZONE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACTIVE_FROM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACTIVE_TO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SL_NUM)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.SET_DIVISION.DIVISION_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZONE_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZONE_CODE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZONE_DETAILS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESC_ZONE)
        </td>
        <td>
            @Html.ValueFor(modelItem => item.TOTAL_STUDENTS, "{0:0}")
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ZONE_GRADE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ACTIVE_ZONE)
        </td>
        <td>
            @Html.ValueFor(modelItem => item.ACTIVE_FROM,"{0:yyyy-MM-dd}")
        </td>
        <td>
            @Html.ValueFor(modelItem => item.ACTIVE_TO, "{0:yyyy-MM-dd}")
        </td>
        <td>
            @Html.ValueFor(modelItem => item.SL_NUM, "{0:0}")
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ZONE_NO },new{@class="btn btn-info"}) |
            @Html.ActionLink("Details", "Details", new { id = item.ZONE_NO }, new { @class = "btn btn-success" }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.ZONE_NO }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
