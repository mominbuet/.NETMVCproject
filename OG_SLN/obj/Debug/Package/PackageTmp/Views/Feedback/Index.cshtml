    });
</script>
<h2>Feedbacks</h2>
@{Html.RenderPartial("_FeedbackSearchPartial");}
     <div class="row">
      <div class="col-8 marRight_2">
        <div class="table-responsive">
            <table class="table table-responsive table-bordered table-hover tabWidth">
                <tbody>
                <tr class="success">
