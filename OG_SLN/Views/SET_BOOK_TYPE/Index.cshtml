@model IEnumerable<OG_SLN.SET_BOOK_TYPE>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { }, new { @class="btn btn-primary"})
</p>
<table class="table table-responsive">
    <tr>
        
        <th>
            Book Type Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESC_BOOK_TYPE)
        </th>
      
        <th>
            Active
        </th>
        <th>
            Active From
        </th>
        <th>
            Active To
        </th>
        <th>
            SL No
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.BOOK_TYPE_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESC_BOOK_TYPE)
        </td>
   
        <td>
            @Html.DisplayFor(modelItem => item.ACTIVE_BOOK_TYPE)
        </td>
        <td>
            @Html.ValueFor(modelItem => item.ACTIVE_FROM, "{0:yyyy-MM-dd}")
        </td>
        <td>
            @Html.ValueFor(modelItem => item.ACTIVE_TO, "{0:yyyy-MM-dd}")
        </td>
        <td>
            @Html.ValueFor(modelItem => item.SL_NUM, "{0:0}")
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.BOOK_TYPE_NO }, new { @class="btn btn-info"}) |
            @Html.ActionLink("Details", "Details", new { id = item.BOOK_TYPE_NO }, new { @class = "btn btn-success" }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.BOOK_TYPE_NO }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
