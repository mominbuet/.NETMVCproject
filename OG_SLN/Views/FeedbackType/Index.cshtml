@model IEnumerable<OG_SLN.SET_FEEDBACK_TYPE>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-responsive table-bordered table-hover tabWidth small-font">
    <tr class="success" style="font-size: 1.1em">
        
        <th>
            @*@Html.DisplayNameFor(model => model.FEEDBACK_NAME)*@
            Feedback Type Name
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.FEEDBACK_CODE)*@
            Feedback Code
        </th>
        <th>
